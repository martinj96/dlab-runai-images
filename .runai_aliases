# RUNAI ALIASES

rpf() {
	kubectl port-forward $1-0-0 2222:22
}
alias ric="runai config cluster ic-caas"
alias rrcp="runai config cluster rcp-caas-test"
alias rl="runai list"
alias rdj="runai delete job"
alias rs="runai submit -i ghcr.io/jkminder/dlab-runai-images/pytorch:master --pvc runai-dlab-{GASPAR_USERNAME}-scratch:/mnt"
alias rb="runai bash"